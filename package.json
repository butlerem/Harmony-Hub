  "bugs": {
    "url": "https://github.com/butlerem/Harmony-Hub/issues"
  },
  "homepage": "https://github.com/butlerem/Harmony-Hub#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "concurrently": "^7.6.0",
    "mongoose": "^7.5.2"
  }
}
